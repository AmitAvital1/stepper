<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.resources.main.AdminAppMainConroller">
   <content>
      <BorderPane>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </padding>
         <top>
            <fx:include fx:id="headerComponent" source="../header/header.fxml" BorderPane.alignment="TOP_CENTER" />
         </top>
         <center>
            <fx:include fx:id="bodyComponent" source="../body/body.fxml" BorderPane.alignment="TOP_CENTER" />
         </center>
      </BorderPane>
   </content>
</ScrollPane>
